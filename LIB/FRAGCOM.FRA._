        AAC2020_FRAGCOM_ABS_F32(
            F32 f32
        )
        {
            #if defined( AAC2020_MACRO_THIS_IS_OPEN_GL )

                return( abs( f32 ) );

            #else
                //:C99 Code Here
                F32 f32_out;
                if( f32 >= 0 ){
