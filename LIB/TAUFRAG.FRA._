        I32
        aac2020_taufrag_pix_dex(    /** @_PRIVATE_FUNC_@ **/
            I32 dex_tau //:EX: AAC2020_TAUDEPO.TAU_015                         
        ,   I32 dex_sub //:EX: AAC2020_TAUDEPO.PAINT5D                                                  
        ,   I32 dex_var //:EX: AAC2020_TAUDEPO.P5D_A_D  
        )
        {
        /** ******************************************** ***
        About this function:
        Original_Name[ aac2020_p5d1ogl_taudepo_pix_dex ]
        CPU_SIDE_CODE[         aac2020_taudepo_pix_dex ]
        *** ******************************************** **/
            #if !defined( AAC2020_MACRO_THIS_IS_OPEN_GL ) //

                //:@VID_IID[ 0177 ]TIME[ 00:10:42 ]
                //:NUTS:Need_Unit_Test
                AAC2020_TODOMAN_Vital(
                    E_S( [NUT:2021_03_13] )
                );;

            #endif //:###################################://

            I32 pix_dex; //:<--[ output/return ]variable

            /** ************************************* **/
            /** ^ is placeholder for ownership symbol **/
            /**   in english language because need to **/
            /**   avoid quotes in this file because   **/
            /**   it gets stringified.                **/
            /**   @VID_IID[ 0177 ]TIME[ 00:54:00 ]    **/
            /** ************************************* **/

            I32 t_x ; //: [dex_tau]^s xy location 
            I32 t_y ; //: [dex_tau]^s xy location 
                     
            I32 s_x ; //: [dex_sub]^s xy location 
            I32 s_y ; //: [dex_sub]^s xy location 
                        
            I32 v_x ; //: [dex_var]^s xy location 
            I32 v_y ; //: [dex_var]^s xy location 

            I32 p_x ; //: pixel_x: (GLOBAL, on 512x512)
            I32 p_y ; //: pixel_y: (GLOBAL, on 512x512)

            //:INDEX_TO_XY_GLSL:-------------------------://  
            //:                                          ://
            //:SEE[ #DIA_TAU_CEL# ]IN[ LIB/TAUDEPO.D._ ] ://
            #define D dex_tau          //:DEX_TAU_000_015://
            t_y  =MI3( TRU( MF3(D) / MF3(   4 )));   
            t_x  =(       (     D  -(t_y *  4 )));                                   
            #undef  D                  //:DEX_TAU_000_015:// 
            //:                                          ://
            //:SEE[ #DIA_SUB_CEL# ]IN[ LIB/TAUDEPO.D._ ] ://
            #define D dex_sub          //:DEX_SUB_000_015://
            s_y  =MI3( TRU( MF3(D) / MF3(   4 )));   
            s_x  =(       (     D  -(s_y *  4 )));                                   
            #undef  D                  //:DEX_SUB_000_015://
            //:                                          ://
            //:SEE[ #DIA_VAR_CEL# ]IN[ LIB/TAUDEPO.D._ ] ://
            #define D dex_var          //:DEX_VAR_000_255://
            v_y  =MI3( TRU( MF3(D) / MF3(  16 )));   
            v_x  =(       (     D  -(v_y * 16 )));                                   
            #undef  D                  //:DEX_VAR_000_255://
            //:                                          ://
            //:-------------------------:INDEX_TO_XY_GLSL://

            //:global_pixel_xy_on_512x512:---------------://
                                                      //:://
            p_x =( 0                                  //:://
            +  (t_x * (512 / 4) ) //: 128x128_tau_cells  ://
            +  (s_x * (128 / 4) ) //: 032x032_sub_cells  ://
            +  (v_x * (    2  ) ) //: 002x002 pix_per_var://
            );;                   //:(002x002:4pixper_var://
                                                      //:://
            p_y =( 0                                  //:://
            +  (t_y * (512 / 4) ) //: 128x128_tau_cells  ://
            +  (s_y * (128 / 4) ) //: 032x032_sub_cells  ://
            +  (v_y * (    2  ) ) //: 002x002 pix_per_var://
            );;                   //:(002x002:4pixper_var://
            //:                     :                    ://
            //:---------------:global_pixel_xy_on_512x512://

            //:XY_TO_INDEX_FORMULA_FOR_TOP_LEFT_PIXEL    ://
            //:OF_OUR_2X2_CLUSTER_OF_PIXELS_REPRESENTING ://
            //:A_SINGLE_VARIABLE.                        ://
                    pix_dex = p_x + ( 512 * p_y);
            return( pix_dex );
        }
