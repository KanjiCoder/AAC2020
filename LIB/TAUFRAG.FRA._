        U32                                          
        aac2020_taufrag_Get_U32(    /** @_PRIVATE_FUNC_@ **/           
            I32 dex_tau //:EX: AAC2020_TAUDEPO.TAU_015                         
        ,   I32 dex_sub //:EX: AAC2020_TAUDEPO.PAINT5D                                                  
        ,   I32 dex_var //:EX: AAC2020_TAUDEPO.P5D_A_D  
        )
        {
            ASS( dex_tau <= ( 16-1) );
            ASS( dex_sub <= ( 16-1) );
            ASS( dex_var <= (256-1) );
                    
            I32 pix_dex; //:GLOBAL_PIXEL_LOCATION: AS_INDEX
            IV2     glo; //:GLOBAL_PIXEL_LOCATION: AS____XY

            pix_dex=(
                aac2020_taufrag_pix_dex(
                    dex_tau //: EX: AAC2020_TAUDEPO.TAU_015
                ,   dex_sub //: EX: AAC2020_TAUDEPO.PAINT5D        
                ,   dex_var //: EX: AAC2020_TAUDEPO.P5D_A_D    
                ));;

            
            //:INDEX_TO_XY_GLSL:-------------------------://                          
            #define D pix_dex  //:GLOBAL:DEX             ://
            #define G glo      //:GLOBAL:XY              ://
            G.y  =MI3( TRU( MF3(D) /  MF3( 512)));   
            G.x  =   (    (     D  - (G.y *512)));                                   
            #undef  D                 //:----------------://             
            #undef  G                 //:----------------://
            //:-------------------------:INDEX_TO_XY_GLSL://

                                                     
            U32 u32_var ;                            
                                                           
            u32_var =(                               
                AAC2020_FRAGCOM_TexelFetch_U32(           
                    TAU //:texture_to_sample    ://  
                ,   glo //:integer_texel_coord  ://  
                ,   I00 //:level_of_detail_zero ://  
                ));;                                 
           
            /** Return Value Encoded In Taudepo Memory ..**/
            /** TAU: Texture_As_Uniform .............. ..**/
            return( u32_var );                       
        }       
        //:- - - - - - - - - - - -- - - - - - - - - - - -://

            //:@VID_IID[ 0201 ]TIME[ 02:45:42 ]          ://
            //:Get: Get a value                          ://
            //:015: From TAU_015 section.                ://
            //:P5D: SubSection: Paint5D variables        ://
            //:VAR: Supply variable's handle.            ://
            //:I32: Return as Int32                      ://
            I32
            AAC2020_TAUFRAG_Get_015_P5D_VAR_I32( 
                I32 var_p5d //:<--paint5d variable to grab.
            )
            {
                U32             val_u32; //:GotValueAs:u32
                I32             val_i32; //:GotValueAs:i32
                                val_u32 =( 
                aac2020_taufrag_Get_U32(
                                TAU_015 //:tau: TAU_015 
                    ,           SUB_P5D //:sub: PAINT5D 
                    ,           var_p5d //:var: P5D_*** 
                ));;            val_i32=( 
                           MI3( val_u32 ) );;
                return(         val_i32   );
            }   

