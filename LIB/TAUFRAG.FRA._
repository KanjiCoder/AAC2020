        I32
        aac2020_taufrag_pix_dex(    /** @_PRIVATE_FUNC_@ **/
            I32 dex_tau //:EX: AAC2020_TAUDEPO.TAU_015                         
        ,   I32 dex_sub //:EX: AAC2020_TAUDEPO.PAINT5D                                                  
        ,   I32 dex_var //:EX: AAC2020_TAUDEPO.P5D_A_D  
        )
        {
        /** ******************************************** ***
        About this function:
        Original_Name[ aac2020_p5d1ogl_taudepo_pix_dex ]
        CPU_SIDE_CODE[         aac2020_taudepo_pix_dex ]
        *** ******************************************** **/
            #if !defined( AAC2020_MACRO_THIS_IS_OPEN_GL ) //

                //:@VID_IID[ 0177 ]TIME[ 00:10:42 ]
                //:NUTS:Need_Unit_Test
                AAC2020_TODOMAN_Vital(
                    E_S( [NUT:2021_03_13] )
                );;

            #endif //:###################################://

            I32 pix_dex; //:<--[ output/return ]variable

            /** ************************************* **/
            /** ^ is placeholder for ownership symbol **/
            /**   in english language because need to **/
            /**   avoid quotes in this file because   **/
            /**   it gets stringified.                **/
            /**   @VID_IID[ 0177 ]TIME[ 00:54:00 ]    **/
            /** ************************************* **/

            I32 t_x ; //: [dex_tau]^s xy location 
            I32 t_y ; //: [dex_tau]^s xy location 
                     
            I32 s_x ; //: [dex_sub]^s xy location 
            I32 s_y ; //: [dex_sub]^s xy location 
                        
            I32 v_x ; //: [dex_var]^s xy location 
            I32 v_y ; //: [dex_var]^s xy location 

            I32 p_x ; //: pixel_x: (GLOBAL, on 512x512)
            I32 p_y ; //: pixel_y: (GLOBAL, on 512x512)

            //:INDEX_TO_XY_GLSL:-------------------------://  
            //:                                          ://
            //:SEE[ #DIA_TAU_CEL# ]IN[ LIB/TAUDEPO.D._ ] ://
            #define D dex_tau          //:DEX_TAU_000_015://
            t_y  =MI3( TRU( MF3(D) / MF3(   4 )));   
            t_x  =(       (     D  -(t_y *  4 )));                                   
            #undef  D                  //:DEX_TAU_000_015:// 
            //:                                          ://
            //:SEE[ #DIA_SUB_CEL# ]IN[ LIB/TAUDEPO.D._ ] ://
            #define D dex_sub          //:DEX_SUB_000_015://
            s_y  =MI3( TRU( MF3(D) / MF3(   4 )));   
            s_x  =(       (     D  -(s_y *  4 )));                                   
            #undef  D                  //:DEX_SUB_000_015://
            //:                                          ://
            //:SEE[ #DIA_VAR_CEL# ]IN[ LIB/TAUDEPO.D._ ] ://
            #define D dex_var          //:DEX_VAR_000_255://
            v_y  =MI3( TRU( MF3(D) / MF3(  16 )));   
            v_x  =(       (     D  -(v_y * 16 )));                                   
            #undef  D                  //:DEX_VAR_000_255://
            //:                                          ://
            //:-------------------------:INDEX_TO_XY_GLSL://

            //:global_pixel_xy_on_512x512:---------------://
                                                      //:://
            p_x =( 0                                  //:://
            +  (t_x * (512 / 4) ) //: 128x128_tau_cells  ://
            +  (s_x * (128 / 4) ) //: 032x032_sub_cells  ://
            +  (v_x * (    2  ) ) //: 002x002 pix_per_var://
            );;                   //:(002x002:4pixper_var://
                                                      //:://
            p_y =( 0                                  //:://
            +  (t_y * (512 / 4) ) //: 128x128_tau_cells  ://
            +  (s_y * (128 / 4) ) //: 032x032_sub_cells  ://
            +  (v_y * (    2  ) ) //: 002x002 pix_per_var://
            );;                   //:(002x002:4pixper_var://
            //:                     :                    ://
            //:---------------:global_pixel_xy_on_512x512://

            //:XY_TO_INDEX_FORMULA_FOR_TOP_LEFT_PIXEL    ://
            //:OF_OUR_2X2_CLUSTER_OF_PIXELS_REPRESENTING ://
            //:A_SINGLE_VARIABLE.                        ://
                    pix_dex = p_x + ( 512 * p_y);
            return( pix_dex );
        }
        //:- - - - - - - - - - - -- - - - - - - - - - - -://


        U32                                          
        aac2020_taufrag_Get_U32(    /** @_PRIVATE_FUNC_@ **/           
            I32 dex_tau //:EX: AAC2020_TAUDEPO.TAU_015                         
        ,   I32 dex_sub //:EX: AAC2020_TAUDEPO.PAINT5D                                                  
        ,   I32 dex_var //:EX: AAC2020_TAUDEPO.P5D_A_D  
        )
        {
            ASS( dex_tau <= ( 16-1) );
            ASS( dex_sub <= ( 16-1) );
            ASS( dex_var <= (256-1) );
                    
            I32 pix_dex; //:GLOBAL_PIXEL_LOCATION: AS_INDEX
            IV2     glo; //:GLOBAL_PIXEL_LOCATION: AS____XY

            pix_dex=(
                aac2020_taufrag_pix_dex(
                    dex_tau //: EX: AAC2020_TAUDEPO.TAU_015
                ,   dex_sub //: EX: AAC2020_TAUDEPO.PAINT5D        
                ,   dex_var //: EX: AAC2020_TAUDEPO.P5D_A_D    
                ));;

            
            //:INDEX_TO_XY_GLSL:-------------------------://                          
            #define D pix_dex  //:GLOBAL:DEX             ://
            #define G glo      //:GLOBAL:XY              ://
            G.y  =MI3( TRU( MF3(D) /  MF3( 512)));   
            G.x  =   (    (     D  - (G.y *512)));                                   
            #undef  D                 //:----------------://             
            #undef  G                 //:----------------://
            //:-------------------------:INDEX_TO_XY_GLSL://

                                                     
            U32 u32_var ;                            
                                                           
            u32_var =(                               
                AAC2020_FRAGCOM_TexelFetch_U32(           
                    TAU //:texture_to_sample    ://  
                ,   glo //:integer_texel_coord  ://  
                ,   I00 //:level_of_detail_zero ://  
                ));;                                 
           
            /** Return Value Encoded In Taudepo Memory ..**/
            /** TAU: Texture_As_Uniform .............. ..**/
            return( u32_var );                       
        }       
        //:- - - - - - - - - - - -- - - - - - - - - - - -://
        REC                                          
        aac2020_taufrag_Get_REC(    /** @_PRIVATE_FUNC_@ **/              
            I32 dex_tau //:EX: AAC2020_TAUDEPO.TAU_015                         
        ,   I32 dex_sub //:EX: AAC2020_TAUDEPO.PAINT5D                                                  
        ,   I32 dex_var //:EX: AAC2020_TAUDEPO.P5D_VP0  
        )
        {  
            ASS( dex_tau <= ( 16-1) );
            ASS( dex_sub <= ( 16-1) );
            ASS( dex_var <= (256-1) );
                    
            I32 pix_dex; //:GLOBAL_PIXEL_LOCATION: AS_INDEX
            I32     g_x; //:GLOBAL_PIXEL_LOCATION: X-coord
            I32     g_y; //:GLOBAL_PIXEL_LOCATION: Y-coord

            pix_dex=( /**#TOP_LEFT_PIXEL_OF_2X2_CLUSTER#**/
                aac2020_taufrag_pix_dex(
                    dex_tau //: EX: AAC2020_TAUDEPO.TAU_015
                ,   dex_sub //: EX: AAC2020_TAUDEPO.PAINT5D        
                ,   dex_var //: EX: AAC2020_TAUDEPO.P5D_A_D    
                ));;

            
            //:INDEX_TO_XY_GLSL:-------------------------://                          
            #define D pix_dex  //:GLOBAL:DEX             ://
            g_y  =MI3( TRU( MF3(D) / MF3( 512)));   
            g_x  =(       (     D  -(g_y *512)));                                   
            #undef  D                 //:----------------://             
            //:-------------------------:INDEX_TO_XY_GLSL://    

                                                     
            REC rec; /** I32 Rectangle **/                         
                               
            #define GET_U32 AAC2020_FRAGCOM_TexelFetch_U32XY        
            #define GET_I32(a,b,c,d)  MI3(GET_U32(a,b,c,d))        

            //:  LOCATION     DATA_STORED_AT_LOCATION
            //:  +---+---+    +---+---+  //:T_L ==> x_0
            //:  |T_L|T_R|    |x_0|x_1|  //:T_R ==> x_1
            //:  +---+---+    +---+---+  //:B_L ==> y_0
            //:  |B_L|B_R|    |y_0|y_1|  //:B_R ==> y_1
            //:  +---+---+    +---+---+
            rec.x_0 =(GET_I32(TAU,g_x+0,g_y+0,I00)); //T_L//     
            rec.x_1 =(GET_I32(TAU,g_x+1,g_y+0,I00)); //T_R//
            rec.y_0 =(GET_I32(TAU,g_x+0,g_y+1,I00)); //B_L//
            rec.y_1 =(GET_I32(TAU,g_x+1,g_y+1,I00)); //B_R//

            #undef  GET_U32
            #undef  GET_I32
            
           
            /** Return Value Encoded In Taudepo Memory ..**/
            /** TAU: Texture_As_Uniform .............. ..**/
            return( rec );      
        }
