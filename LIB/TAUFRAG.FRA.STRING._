"            }   \n"
"        //:- - - - - - - - - - - -- - - - - - - - - - - -://\n"
"        REC                                          \n"
"        aac2020_taufrag_Get_REC(    /** @_PRIVATE_FUNC_@ **/              \n"
"            I32 dex_tau //:EX: AAC2020_TAUDEPO.TAU_015                         \n"
"        ,   I32 dex_sub //:EX: AAC2020_TAUDEPO.PAINT5D                                                  \n"
"        ,   I32 dex_var //:EX: AAC2020_TAUDEPO.P5D_VP0  \n"
"        )\n"
"        {  \n"
"            ASS( dex_tau <= ( 16-1) );\n"
"            ASS( dex_sub <= ( 16-1) );\n"
"            ASS( dex_var <= (256-1) );\n"
"            I32 pix_dex; //:GLOBAL_PIXEL_LOCATION: AS_INDEX\n"
"            I32     g_x; //:GLOBAL_PIXEL_LOCATION: X-coord\n"
"            I32     g_y; //:GLOBAL_PIXEL_LOCATION: Y-coord\n"
"            pix_dex=( /**#TOP_LEFT_PIXEL_OF_2X2_CLUSTER#**/\n"
"                aac2020_taufrag_pix_dex(\n"
"                    dex_tau //: EX: AAC2020_TAUDEPO.TAU_015\n"
"                ,   dex_sub //: EX: AAC2020_TAUDEPO.PAINT5D        \n"
"                ,   dex_var //: EX: AAC2020_TAUDEPO.P5D_A_D    \n"
"                ));;\n"
"            //:INDEX_TO_XY_GLSL:-------------------------://   \n"
"            #define A MAK_I32 /**MI3**/             //:--://\n"
"            #define B TRU_F32 /**TRU**/             //:--://\n"
"            #define C MAK_F32 /**MF3**/             //:--://\n"
"            #define D pix_dex  //:GLOBAL:DEX        //:--://\n"
"            g_y  = A( B( C(D) / (   C( 512))  ));   //:--://\n"
"            g_x  =  (  (   D  - ((g_y *512))  ));   //:--://                        \n"
"            #undef  A                 //:----------------://             \n"
"            #undef  B                 //:----------------://             \n"
"            #undef  C                 //:----------------://             \n"
"            #undef  D                 //:----------------://             \n"
"            //:-------------------------:INDEX_TO_XY_GLSL://    \n"
"            //:LOAD_PIXEL_DATA_INTO_RECTANGLE:-----------://\n"
"            REC rec; /** I32 Rectangle **/       //:-----://                \n"
"                                                 //:-----://\n"
"            #define       C MAK_I32 /**C:CAST**/ //:-----://\n"
"            #define GET_U32 AAC2020_FRAGCOM_TexelFetch_U32XY            \n"
"            #define GET_I32(a,b,c,d)  C(GET_U32(a,b,c,d))   \n"
"            #define       Z (0) /** Level_Of_Detail == 0 **/\n"
"            //:  LOCATION     DATA_STORED_AT_LOCATION\n"
"            //:  +---+---+    +---+---+  //:T_L ==> x_0\n"
"            //:  |T_L|T_R|    |x_0|x_1|  //:T_R ==> x_1\n"
"            //:  +---+---+    +---+---+  //:B_L ==> y_0\n"
"            //:  |B_L|B_R|    |y_0|y_1|  //:B_R ==> y_1\n"
"            //:  +---+---+    +---+---+\n"
"            rec.x_0 =(GET_I32(TAU,g_x+0,g_y+0, Z )); //T_L//         \n"
"            rec.x_1 =(GET_I32(TAU,g_x+1,g_y+0, Z )); //T_R//\n"
"            rec.y_0 =(GET_I32(TAU,g_x+0,g_y+1, Z )); //B_L//\n"
"            rec.y_1 =(GET_I32(TAU,g_x+1,g_y+1, Z )); //B_R//\n"
"            #undef        C  //:-------------------------://\n"
"            #undef  GET_U32  //:-------------------------://\n"
"            #undef  GET_I32  //:-------------------------://\n"
"            #undef        Z  //:-------------------------://\n"
"            //:-----------:LOAD_PIXEL_DATA_INTO_RECTANGLE://\n"
"            /** Return Value Encoded In Taudepo Memory ..**/\n"
"            /** TAU: Texture_As_Uniform .............. ..**/\n"
"            return( rec );      \n"
"        }\n"
